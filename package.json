  "description": "Nuxt.js + Vuetify.js project",
  "author": "Daniel Torres (danieljtorres94@gmail.com)",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch server_middleware --exec \"cross-env NODE_ENV=development HOST=0.0.0.0 PORT=3000 nuxt\"",
